{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "jsx": "react-jsx",
        "checkJs": false,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}